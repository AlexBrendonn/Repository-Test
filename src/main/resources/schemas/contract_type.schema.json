{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contract Type",
  "description": "Schema contract type",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "description": {
      "type": "string",
      "maxLength": 60
    },
    "type": {
      "type": "string"
    },
    "storage": {
      "type": "boolean"
    },
    "lease": {
      "type": "boolean"
    }
  },
  "maxProperties": 5,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "description",
    "type"
  ]
}