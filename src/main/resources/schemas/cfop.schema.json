{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CFOP",
  "description": "Schema cfop",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "description": {
      "type": "string",
      "maxLength": 60
    },
    "code": {
      "type": "string",
      "maxLength": 4
    }
  },
  "maxProperties": 3,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "description",
    "code"
  ]
}