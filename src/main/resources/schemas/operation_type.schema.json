{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Operation Type",
  "description": "Schema operation type",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "description": {
      "type": "string",
      "maxLength": 80
    },
    "mask": {
      "type": "string",
      "maxLength": 60
    },
    "document_type": {
      "type": "string",
      "maxLength": 10
    },
    "emission_type": {
      "type": "string",
      "maxLength": 11
    },
    "sped": {
      "type": "string",
      "maxLength": 10
    },
    "note": {
      "type": "string",
      "maxLength": 2000
    }
  },
  "maxProperties": 7,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "description",
    "mask",
    "document_type",
    "emission_type"
  ]
}