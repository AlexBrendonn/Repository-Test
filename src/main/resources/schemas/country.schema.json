{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Country",
  "description": "Schema country",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 60
    },
    "code": {
      "type": "string"
    }
  },
  "maxProperties": 3,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "name",
    "code"
  ]
}