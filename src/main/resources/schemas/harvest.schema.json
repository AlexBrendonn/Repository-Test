{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Harvest",
  "description": "Schema harvest",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "description": {
      "type": "string",
      "maxLength": 60
    },
    "mask": {
      "type": "string",
      "maxLength": 20
    },
    "init": {
      "type": "string"
    },
    "end": {
      "type": "string"
    },
    "activated": {
      "type": "boolean",
      "default": false
    }
  },
  "maxProperties": 6,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "description",
    "mask",
    "activated"
  ]
}