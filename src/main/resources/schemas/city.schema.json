{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "City",
  "description": "Schema city",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string",
      "maxLength": 60,
      "minLength": 2
    },
    "code": {
      "type": "string"
    },
    "state": {
      "description": "state state",
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    }
  },
  "maxProperties": 4,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "name",
    "code",
    "state"
  ]
}