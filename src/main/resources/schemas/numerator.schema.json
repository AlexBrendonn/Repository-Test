{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Numerator",
  "description": "Schema numerator",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "person": {
      "type": "integer"
    },
    "sequence": {
      "type": "integer"
    },
    "next_value": {
      "type": "integer"
    },
    "sped": {
      "type": "string"
    }
  },
  "maxProperties": 5,
  "propertyNames": {
    "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
  },
  "required": [
    "person",
    "sped",
    "sequence",
    "next_value"
  ]
}